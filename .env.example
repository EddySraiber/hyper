# Enhanced News Scraping API Keys
# Copy this file to .env and add your actual API keys

# Alpha Vantage (Free tier: 5 requests/minute, 500 requests/day)
# Sign up at: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=demo

# Finnhub (Free tier: 60 calls/minute)
# Sign up at: https://finnhub.io/register
FINNHUB_API_KEY=your_finnhub_api_key_here

# IEX Cloud (Free tier: 50,000 requests/month)
# Sign up at: https://iexcloud.io/console/
IEX_CLOUD_API_KEY=your_iex_cloud_api_key_here

# Alpaca Trading API (Paper Trading)
ALPACA_API_KEY=your_alpaca_api_key
ALPACA_SECRET_KEY=your_alpaca_secret_key
ALPACA_PAPER_TRADING=true

# Binance Trading API (Testnet)
BINANCE_API_KEY=your_binance_testnet_api_key
BINANCE_SECRET_KEY=your_binance_testnet_secret_key

# Application Settings
LOG_LEVEL=INFO